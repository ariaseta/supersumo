# Supabase Configuration
# Get these values from: https://app.supabase.com
# 1. Create a new Supabase project
# 2. Go to Project Settings -> API
# 3. Copy the Project URL and Anon Key
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Feature Flags
VITE_ENABLE_DASHBOARD=true
VITE_ENABLE_AI=true
VITE_ENABLE_CONTAINERS=true
VITE_ENABLE_VPS=true
VITE_ENABLE_DOMAIN=true
VITE_ENABLE_DATABASE=true
VITE_ENABLE_BILLING=true
VITE_ENABLE_AFFILIATE=true
VITE_ENABLE_SETTING=true
VITE_CURRENCY=Rp
